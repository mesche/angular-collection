{
  "name": "angular4-example-dynamic-form-and-validation",
  "version": "1.0.0",
  "license": "SEE LICENSE IN LICENSE.txt",
  "description": "Angular 4 - a web framework for modern web apps",
  "author": "Markus Eschenbach <mail@blogging-it.com> (http://www.blogging-it.com)",
  "homepage": "https://github.com/mesche/angular-collection",
  "bugs": "https://github.com/mesche/angular-collection/issues",
  "contributors": [
    "Markus Eschenbach <mail@blogging-it.com> (http://www.blogging-it.com)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mesche/angular-collection.git"
  },
  "scripts": {
    "ng": "ng",
    "rimraf": "rimraf",
    "typedoc": "typedoc",
    "clang-format": "clang-format",
    "webdriver-manager": "webdriver-manager",
    "protractor": "protractor",
    "karma": "karma",
    "inst": "npm install",
    "start": "ng serve",
    "build": "ng build",
    "build_prod": "ng build --prod",
    "build_prod_jit": "ng build --prod --aot false",
    "test": "ng test",
    "lint": "ng lint",
    "format": "npm run clang-format -- -i -style=file --glob=src/**/*.ts",
    "pree2e": "npm run webdriver-manager -- update",
    "e2e": "ng e2e",
    "docs": "npm run typedoc -- --options typedoc.json ./src/",
    "ci": "ng lint && ng test && ng e2e",
    "clean": "npm cache clean && npm run rimraf -- dist doc compiled coverage dll node_modules",
    "clean_dist": "npm run rimraf -- dist",
    "set_noprog": "npm set progress=false",
    "set_yarn": "ng set --global packageManager=yarn",
    "set_npm": "ng set --global packageManager=npm"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4",
    "class-transformer": "^0.1.6",
    "class-validator": "^0.6.8",
    "reflect-metadata": "^0.1.10"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0",
    "clang-format": "^1.0.48",
    "typedoc": "^0.5.7",
    "webdriver-manager": "^12.0.3"
  }
}