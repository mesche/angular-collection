{
  "name": "angular4-quickstart-cli",
  "version": "1.0.0",
  "license": "SEE LICENSE IN LICENSE.txt",
  "description": "Angular 4 - a web framework for modern web apps",
  "author": "Markus Eschenbach <mail@blogging-it.com> (http://www.blogging-it.com)",
  "homepage": "https://github.com/mesche/angular-collection",
  "bugs": "https://github.com/mesche/angular-collection/issues",
  "contributors": [
    "Markus Eschenbach <mail@blogging-it.com> (http://www.blogging-it.com)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mesche/angular-collection.git"
  },
  "scripts": {
    "ng": "ng",
    "tsc": "tsc",
    "rimraf": "rimraf",
    "compodoc": "compodoc",
    "tslint": "tslint",
    "clang-format": "clang-format",
    "webdriver-manager": "webdriver-manager",
    "protractor": "protractor",
    "karma": "karma",
    "tsc:w": "tsc -w",
    "inst": "npm install",
    "start": "ng serve",
    "build": "ng build",
    "build_prod": "ng build --prod",
    "build_prod_jit": "ng build --prod --aot false",
    "test": "ng test",
    "lint": "ng lint",
    "format": "npm run clang-format -- -i -style=file --glob=src/**/*.ts",
    "pree2e": "npm run webdriver-manager -- update",
    "e2e": "ng e2e",
    "docs": "npm run compodoc -- -p tsconfig.json --hideGenerator  -d ./dist/docs/",
    "clean": "npm cache clean && npm run rimraf -- target dist doc compiled coverage dll node_modules",
    "clean_dist": "npm run rimraf -- ./dist",
    "clean_reports": "npm run rimraf -- ./dist/reports",
    "set_noprog": "npm set progress=false",
    "set_yarn": "ng set --global packageManager=yarn",
    "set_npm": "ng set --global packageManager=npm",
    "ci-test": "ng test --watch false --single-run true",
    "ci": "ng lint && ng test && ng e2e",
    "report-test-coverage": "npm test -- --watch false --single-run true --code-coverage",
    "report-lint": "ng lint --force true --format json > ./dist/reports/lint.json",
    "report-all": "npm run report-test-coverage && npm run report-lint",
    "dist": "npm run clean_dist && npm run ci-test && ng e2e && npm run build && npm run report-all && npm run docs"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@compodoc/compodoc": "^1.0.0-beta.3",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "clang-format": "^1.0.49",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "protractor": "~5.1.0",
    "rimraf": "^2.6.1",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0",
    "webdriver-manager": "^12.0.4"
  }
}
